	<typeHandlers>
        <typeHandler handler="com.kg.fin.handler.MyLongTypeHandler" jdbcType="LONGVARCHAR" javaType="java.lang.String"/>
	</typeHandlers>
	
	<mappers>
		
		<mapper resource="mapper/member/member.xml" />
		<mapper resource="mapper/buyer/buyer.xml" />
		<mapper resource="mapper/map/map.xml" />
		<mapper resource="mapper/manager/manager.xml" />
		<mapper resource="mapper/search/search.xml" />
		<mapper resource="mapper/seller/seller.xml" />
		<mapper resource="mapper/serviceCenter/serviceCenter.xml" />
