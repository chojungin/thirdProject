		<typeAlias type="java.util.HashMap" alias="java.util.HashMap"></typeAlias> 
		<typeAlias type="com.kg.fin.buyer.vo.BuyerVO" alias="buyerVO" />
		<typeAlias type="com.kg.fin.seller.vo.SellerVO" alias="sellerVO"/>
		<typeAlias type="com.kg.fin.sellImg.vo.SellImgVO" alias="sellImgVO"/>
		<typeAlias type="com.kg.fin.sellPlace.vo.SellPlaceVO" alias="sellPlaceVO"/>
		<typeAlias type="com.kg.fin.sellComment.vo.SellCommentVO" alias="sellCommentVO"/>
